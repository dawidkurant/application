{
  "ConnectionStrings": {
    "PapuDbConnection": "Server=(localdb)\\mssqllocaldb;Database=PapuDb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Key": {
      "Type": "Development"
    }
  },
  "AllowedHosts": "*",
  "StaticFiles": {
    "Headers": {
      "CacheControl": "no-cache, no-store",
      "Pragma": "no-cache",
      "Expires": "-1"
    }
  }
}
